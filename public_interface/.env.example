# TradeOracle Nexus - Configuration
# Copy this file to .env and fill in your values

# Google Gemini API Key (required)
GOOGLE_API_KEY=your_gemini_api_key_here

# Elasticsearch Serverless (required for Agent Builder)
ELASTICSEARCH_URL=https://your-cluster.es.us-east-1.aws.elastic.cloud
ELASTICSEARCH_API_KEY=your_elasticsearch_api_key

# MEXC Futures API (optional - for live market data)
MEXC_ACCESS_KEY=your_mexc_access_key
MEXC_SECRET_KEY=your_mexc_secret_key

# Telegram Bot (optional - for alerts)
TELEGRAM_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# LM Studio Cluster (optional - for local AI consensus)
LM_STUDIO_M1_URL=http://localhost:1234
LM_STUDIO_M2_URL=http://localhost:1235
LM_STUDIO_M3_URL=http://localhost:1236
